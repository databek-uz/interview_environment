{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug PySpark (Bitnami)",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
        "SPARK_HOME": "/opt/bitnami/spark",
        "PYSPARK_PYTHON": "/opt/bitnami/python/bin/python"
      }
    }
  ]
}
